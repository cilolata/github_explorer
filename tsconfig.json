{
  "compilerOptions": {
    "target": "es2016",                                  
    "lib": ["DOM" , "DOM.Iterable" , "esnext"],                                        
    "jsx": "react-jsx",                                
    "module": "commonjs",                                
    "moduleResolution": "node",                      
    "resolveJsonModule": true,                       
     "allowJs": true,                                
    
    "noEmit": true,                                   
    "isolatedModules": true,                          
    "allowSyntheticDefaultImports": true,             
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    "skipLibCheck": true                                 
  }
}
